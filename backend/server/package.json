{
  "name": "@fetchr/backend-server",
  "license": "MIT",
  "type": "module",
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">= 8.3.0",
    "node": "20.12.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "runall": "NODE_ENV=development run-p watch watch-proto watch-shared run-python-server",
    "runall-no-lint": "NODE_ENV=development run-p watch-no-lint watch-proto watch-shared run-python-server",
    "open-redis-tunnel": "ssh -i ../certificates/shared-fetchr-key.pem -f  -N -L 6381:clustercfg.fetchr-redis.l1x4on.memorydb.us-west-2.amazonaws.com:6379 ubuntu@ec2-35-90-17-82.us-west-2.compute.amazonaws.com",
    "watch": "npm-run-all watch-src watch-shared",
    "watch-no-lint": "npm-run-all watch-src-no-lint watch-shared",
    "watch-src": "nodemon --watch src -e ts,tsx --exec \"pnpm run lint && NODE_PATH=../packages dotenv -e .env tsx\" src",
    "watch-src-no-lint": "nodemon --watch src -e ts,tsx --exec \"NODE_PATH=../packages dotenv -e .env tsx\" src",
    "watch-shared": "nodemon --watch src/shared -e ts --exec \"pnpm run generate-shared\"",
    "generate-shared": "npx tsx copySharedCode.ts src/shared/converters.ts",
    "watch-proto": "pnpm run proto && nodemon --on-change-only --watch '../../schema/**/*' --ext proto --exec 'pnpm run proto'",
    "run-python-server": "cd ../python_server && (command -v lsof >/dev/null && lsof -i :9091 || true) && python3 main.py",
    "build": "pnpm run lint && pnpm run build-server",
    "build-server": "node esbuild.config.mjs",
    "build-stagehand": "cd ../packages/stagehand && pnpm build",
    "start": "NODE_ENV=production run-p run-python-server start-helper",
    "start-helper": "NODE_PATH=../packages dotenv -e .env.production tsx src/index.ts",
    "proto": "mkdir -p src/proto && protoc --experimental_allow_proto3_optional --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/proto --ts_proto_opt=outputServices=generic-definitions,outputServices=nice-grpc,outputClientImpl=false,oneof=unions,snakeToCamel=true,esModuleInterop=true,lowerCaseServiceMethods=true,useExactTypes=true,outputPartialMethods=false --proto_path=../../schema ../../schema/**/*.proto",
    "script": "dotenv -e .env -- npx tsx",
    "db:update": "pnpm run prisma:pull && pnpm run prisma:generate",
    "prisma:studio": "prisma studio",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "type-check": "tsc --noEmit",
    "postinstall": "cd ../python_server && pip3 install -r requirements.txt",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
